<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="minimalistbrowser">
	<schema id="in.aryank.MinimalistBrowser" path="/in/aryank/MinimalistBrowser/">
        <key type="s" name="search-engine">
            <default>"https://www.duckduckgo.com/?q={url}"</default>
        </key>
        <key type="b" name="search-engine-duck">
            <default>true</default>
            <summary>DuckDuckGo radio button state</summary>
        </key>
        <key type="b" name="search-engine-google">
            <default>false</default>
            <summary>Google radio button state</summary>
        </key>
        <key type="b" name="search-engine-custom">
            <default>false</default>
            <summary>Custom search engine radio button state</summary>
        </key>
        <key type="b" name="custom-homepage">
            <default>false</default>
            <summary>Custom homepage radio button state</summary>
        </key>
        <key type="s" name="custom-homepage-url">
            <default>""</default>
            <summary>Stores custom homepage URL</summary>
        </key>
	</schema>
</schemalist>